*t5

.include General_Circuit.cir

.op
.end

.control

*makes plots in color
set hcopypscolor=0
set color0=white
set color1=black
set color2=red
set color3=blue
set color4=violet
set color5=rgb:3/8/0
set color6=rgb:4/0/0

print all

** 	Time Analysis		**
tran 1e-5 11.5e-3 1.5e-3

hardcopy vo1t.ps v(out) v(in)
echo vo1t_FIG

** 	Frequency Analysis	**
ac dec 10 10 100MEG

*print all

hardcopy vodb.ps vdb(out) vecmax(vdb(out))-3
echo vodb_FIG

print vecmax(vdb(out))

let lower_bond 	= vecmax(vdb(out))-3
let amplitude		= abs(vecmax(v(out)))
let amplitude_vdb	= vecmax(vdb(out))

let normal_cost	= 2*220*0.001 + 3*10 + 3*1 + 3*100
let cost		= normal_cost +13.323*1000

echo "SIM_DATA_TAB"

meas ac f1 WHEN vdb(out)=lower_bond RISE=1
meas ac f2 WHEN vdb(out)=lower_bond CROSS=LAST

let	f0 = sqrt(f1*f2)

print f0
print f0 - 1000

print amplitude_vdb
print amplitude
print amplitude - 100

let merit		= abs( 1 / ( cost * ((f0 - 1000) + (amplitude -100) + 1e-6) ) )
let normal_merit	= abs( 1 / ( normal_cost * (f0 - 1000) * (abs(vecmax(v(out))) - 100) ) )

print cost
print merit
*print normal_merit

echo "SIM_DATA_END"

**	Input Impedance	**
let Zin = v(in)[20]/ (v(input_r)[20] / 1k)

echo "SIM_ZIN_TAB"

*input impedance in ohm
print Zin

echo "SIM_ZIN_END"


quit
.endc

.end

